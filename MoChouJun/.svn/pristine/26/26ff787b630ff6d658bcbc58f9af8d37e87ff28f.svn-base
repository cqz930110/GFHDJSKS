#import "PSBuddy.h"

@implementation PSBuddy
- (NSString *)reviewName
{
    if (_notes.length > 0)
    {
        return _notes;
    }
    else if (_nickName.length > 0)
    {
        return _nickName;
    }
    else
    {
        return _userName;
    }
}

@end
