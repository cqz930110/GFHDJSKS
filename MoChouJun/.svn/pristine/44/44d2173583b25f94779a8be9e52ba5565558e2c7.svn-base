#import <Foundation/Foundation.h>

@interface CrowdFundingObj : NSObject

/// 项目Id
@property (nonatomic,assign)int Id;

/// 众筹名称
@property (nonatomic,strong)NSString *name;

/// 图片地址，多张图片逗号分割
@property (nonatomic,strong)NSString *images;

/// 期限,单位：天
@property (nonatomic,assign)int remainDays;

/// 创建日期
@property (nonatomic,copy)NSString *createDate;

/// 项目发布人的昵称
@property (nonatomic,copy)NSString *nickName;

/// 项目发布人的图像
@property (nonatomic,copy)NSString *avatar;

/// 该项目被支持的人数
@property (nonatomic,assign)int supportedCount;

/// 被评论人数
@property (nonatomic,assign)int commentedCount;

/// 目标金额
@property (assign,nonatomic)int targetAmount;

/// 已筹金额
@property (assign,nonatomic)double raisedAmount;

/// 简介
@property (nonatomic,strong)NSString *profile;

@property (nonatomic,strong)NSString *userName;

//   剩余天数，状态
@property (nonatomic,strong)NSString *showStatus;
@end
